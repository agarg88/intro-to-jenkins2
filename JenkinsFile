pipeline {
    agent {
        label 'docker' 
    }
  stages {
    stage('Docker node test') {
      agent {
        docker {
          label 'docker'
          image 'node:7-alpine'
          args '--name docker-node' // list any args
        }
      }
      steps {
        sh 'node --version'
      }
    }

   stage('build') {
            steps {
                sh 'mvn --version'
            }
   }
 }
}
}

